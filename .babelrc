{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "last 2 versions",
          "> 0.5%"
        ]
      }
    }],
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    "webpack-chunkname"
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "stage-0"
      ],
      "plugins": ["istanbul"]
    }
  }
}
